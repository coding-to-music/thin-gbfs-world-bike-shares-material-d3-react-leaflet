{
  "name": "thin-gbfs",
  "version": "1.0.0",
  "description": "A fast and low-network-use mobile web app for many common bike sharing systems.",
  "author": "yuzawa-san",
  "license": "MIT",
  "dependencies": {
    "compass.js": "ai/compass.js",
    "d3-color": "^1.0.3",
    "emoji-flags": "^1.2.0",
